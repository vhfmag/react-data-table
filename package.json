{
  "name": "cube-data-table",
  "version": "0.0.18",
  "description": "Yet another react data table component",
  "license": "MIT",
  "scripts": {
    "prepare": ". ./.scripts/prepublish.sh",
    "lint": "tslint src",
    "lintfix": "tslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner compiled/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@storybook/addon-options": "^3.2.13",
    "@types/chai": "^4.0.4",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.0",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/faker": "^4.1.0",
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.79",
    "@types/node": "^8.0.46",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^16",
    "@types/reselect": "^2.2.0",
    "@types/sinon": "^2.3.3",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-fast-async": "^6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-runtime": "^6.25.0",
    "chai": "^4.1.1",
    "classnames": "^2.2.5",
    "core-decorators": "^0.19.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "faker": "^4.1.0",
    "file-saver": "^1.3.3",
    "git-url-parse": "^7.0.0",
    "jest": "^21.2.1",
    "jsdom": "^11.1.0",
    "lodash": "^4.17.4",
    "moize": "^3.3.1",
    "node-sass": "^4.5.3",
    "postcss-modules": "^0.8.0",
    "proptypes": "^1.1.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-lazyload": "^2.2.7",
    "reselect": "^3.0.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "sass-loader": "^6.0.6",
    "sinon": "^3.1.0",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.1.3",
    "ts-node": "^3.3.0",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typelevel-ts": "^0.2.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.2"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "dependencies": {
    "@types/react-lazyload": "^2.2.3"
  },
  "engines": {
    "npm": "^3.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.es6.js",
  "modules": "dist/index.es6.js",
  "jsnext:main": "dist/index.es6.js",
  "types": "./dist/declarations/index.d.ts",
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/tests/"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/src/tests/.*\\.spec\\.(ts|tsx|js|jsx)$"
  }
}
